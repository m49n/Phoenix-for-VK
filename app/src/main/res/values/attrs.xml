<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WrapWidthTextView">
        <attr name="fixWrapText" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomPreferenceCategory">
        <attr name="category_icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomAuthorPreference">
        <attr name="avatar" format="reference" />
    </declare-styleable>


    <declare-styleable name="CircleRoadProgressWidget">
        <attr name="circleCenterPointX" format="float" />
        <attr name="circleCenterPointY" format="float" />

        <attr name="roadColor" format="color" />
        <attr name="roadStrokeWidth" format="dimension" />
        <attr name="roadRadius" format="float" />

        <attr name="arcLoadingColor" format="color" />
        <attr name="arcLoadingStrokeWidth" format="dimension" />
        <attr name="arcLoadingStartAngle" format="float" />
    </declare-styleable>

    <declare-styleable name="CircleImageButton">
        <attr name="backgroundColor" format="color" />
        <attr name="iconColor" format="color" />
    </declare-styleable>

    <declare-styleable name="Emojicon">
        <attr name="displayHashTags" format="boolean" />
        <attr name="emojiconSize" format="dimension" />
        <attr name="emojiconTextStart" format="integer" />
        <attr name="emojiconTextLength" format="integer" />
        <attr name="hashTagColor" format="color" />
    </declare-styleable>

    <attr name="navi_header_image" format="reference" />

    <attr name="message_bubble_color" format="color" />
    <attr name="dialogs_unread_color" format="color" />
    <attr name="toast_background" format="color" />
    <attr name="icon_color_active" format="color" />
    <attr name="icon_color_inactive" format="color" />

    <attr name="fab_secondary" format="reference" />
    <attr name="fab_player" format="reference" />

    <attr name="navi_menu_color" format="color" />
    <attr name="navi_menu_selector" format="reference" />
    <attr name="messages_background_color" format="color" />
    <attr name="my_messages_bubble_color" format="color" />
    <attr name="chat_background_cookies" format="reference" />
    <attr name="chat_background_lines" format="reference" />
    <attr name="chat_background_runes" format="reference" />
    <attr name="message_unread_color" format="color" />
    <attr name="buttonAddPhotoBackground" format="reference" />
    <attr name="dividerColor" format="color" />

    <attr name="toolbarElevation" format="reference" />
    <attr name="statusbarNonColoredColor" format="color" />

    <!-- QuickReply-->
    <attr name="quick_reply_primary_color" format="color" />
    <attr name="quick_reply_icon_color" format="color" />


    <declare-styleable name="MozaikLayout">
        <attr name="maxSingleImageHeight" format="dimension" />
        <attr name="prefImageSize" format="dimension" />
        <attr name="spacing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AspectRatioFrameLayout">
        <attr name="aspectRatioHeight" format="integer" />
        <attr name="aspectRatioWidth" format="integer" />
    </declare-styleable>

    <declare-styleable name="CircleCounterButton">
        <attr name="button_icon" format="reference" />
        <attr name="always_counter" format="boolean" />
        <attr name="active" format="boolean" />
        <attr name="count" format="integer" />
        <attr name="active_icon_color" format="color" />
        <attr name="noactive_icon_color" format="color" />
        <attr name="active_background_color" format="color" />
        <attr name="noactive_background_color" format="color" />
        <attr name="text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="AlternativeAspectRatioFrameLayout">
        <attr name="altAspectRatioWidth" format="integer" />
        <attr name="altAspectRatioHeight" format="integer" />
    </declare-styleable>

    <declare-styleable name="WaveFormView">
        <attr name="waveform_active_color" format="color" />
        <attr name="waveform_noactive_color" format="color" />
    </declare-styleable>

    <declare-styleable name="KeyboardView">
        <attr name="keyboard_theme" format="enum">
            <enum name="fullscreen" value="0" />
            <enum name="dialog" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ProgressButton">
        <attr name="button_layout" format="reference" />
        <attr name="button_color" format="integer" />
        <attr name="button_text" format="string" />
        <attr name="button_all_caps" format="boolean" />
    </declare-styleable>

    <declare-styleable name="OnlineView">
        <attr name="circle_color" format="color" />
        <attr name="stroke_color" format="color" />
        <attr name="stroke_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ColorFilterImageView">
        <attr name="filter_color" format="color" />
    </declare-styleable>

    <declare-styleable name="SawView">
        <attr name="sawToothPrefWidht" format="dimension" />
        <attr name="sawBackground" format="color" />
    </declare-styleable>

    <declare-styleable name="YoutubeButton">
        <attr name="youtube_button_icon" format="reference" />
        <attr name="youtube_button_text" format="string" />
        <attr name="youtube_button_text_color" format="color" />
        <attr name="youtube_button_icon_color" format="color" />
    </declare-styleable>

    <declare-styleable name="MySpinnerView">
        <attr name="spinner_hint" format="string" />
        <attr name="spinner_hint_color" format="color" />
        <attr name="spinner_text_color" format="color" />
        <attr name="spinner_icon_color" format="color" />
    </declare-styleable>

    <declare-styleable name="AspectRatioImageView">
        <attr name="aspectRatioW" format="integer" />
        <attr name="aspectRatioH" format="integer" />
        <attr name="aspectRatioEnabled" format="boolean" />
        <attr name="dominantMeasurement">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="BottomSheetLayout">
        <attr name="collapsedHeight" format="dimension" />
    </declare-styleable>
</resources>
